<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.worldpay.access.checkout">

    <application
            android:name="com.worldpay.access.checkout.DemoApplication"
            android:allowBackup="true"
            android:icon="@mipmap/wp_logo"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/wp_logo"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:networkSecurityConfig="@xml/network_security_config">
        <uses-library android:name="org.apache.http.legacy" android:required="false" />
        <activity android:name="com.worldpay.access.checkout.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivityJavaExample">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:name=".CVVActivity">

        </activity>
    </application>
</manifest>